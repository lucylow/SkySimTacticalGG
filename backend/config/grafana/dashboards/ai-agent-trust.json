{
  "dashboard": {
    "title": "AI Agent Trust & Review",
    "tags": ["ai", "trust", "review"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Signals Generated per Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_signals_total[5m])",
            "legendFormat": "{{agent}} - {{type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "% Approved vs Rejected",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (agent_signals_total)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Average Confidence vs Approval",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_confidence_avg",
            "legendFormat": "{{agent}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Review Backlog",
        "type": "graph",
        "targets": [
          {
            "expr": "review_queue_depth",
            "legendFormat": "Pending"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Reviewer Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(review_completed_total[5m])",
            "legendFormat": "Reviews/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Approval Rate by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "approval_rate",
            "legendFormat": "{{agent}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}


